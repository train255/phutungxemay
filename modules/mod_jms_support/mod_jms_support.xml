			<field name="yahoo5" type="text" default="" label="JMS_SUPPORT_5_YAHOO" description="JMS_SUPPORT_5_YAHOO_DESC"></field>			
			<field name="skype5" type="text" default="" label="JMS_SUPPORT_5_SKYPE" description="JMS_SUPPORT_5_SKYPE_DESC"></field>
			<field name="aim5" type="text" default="" label="JMS_SUPPORT_5_AIM" description="JMS_SUPPORT_5_AIM_DESC"></field>
			<field name="msn5" type="text" default="" label="JMS_SUPPORT_5_MSN" description="JMS_SUPPORT_5_MSN_DESC"></field>
			<field name="icq5" type="text" default="" label="JMS_SUPPORT_5_ICQ" description="JMS_SUPPORT_5_ICQ_DESC"></field>
			<field name="tel5" type="text" default="" label="JMS_SUPPORT_5_TELEPHONE" description="JMS_SUPPORT_5_TELEPHONE_DESC"></field>
			<field name="email5" type="text" default="" label="JMS_SUPPORT_5_EMAIL" description="JMS_SUPPORT_5_EMAIL_DESC"></field>		
		</fieldset>
		
		<fieldset
				name="advanced">
				<field
					name="moduleclass_sfx"
					type="text"
					label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
					description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />

				<field
					name="cache"
					type="list"
					default="1"
					label="COM_MODULES_FIELD_CACHING_LABEL"
					description="COM_MODULES_FIELD_CACHING_DESC">
					<option
						value="1">JGLOBAL_USE_GLOBAL</option>
					<option
						value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>
				</field>

				<field
					name="cache_time"
					type="text"
					default="900"
					label="COM_MODULES_FIELD_CACHE_TIME_LABEL"
					description="COM_MODULES_FIELD_CACHE_TIME_DESC" />
				<field
					name="cachemode"
					type="hidden"
					default="static">
					<option
						value="static"></option>
				</field>
		</fieldset>	
	</fields>
</config>
