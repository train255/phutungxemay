			<field name="support2" type="text" default="" label="JMS_SUPPORT_2_SUPPORT_NAME" description="JMS_SUPPORT_2_SUPPORT_NAME_DESC"></field>
			<field name="yahoo2" type="text" default="" label="JMS_SUPPORT_2_YAHOO" description="JMS_SUPPORT_2_YAHOO_DESC"></field>			
			<field name="skype2" type="text" default="" label="JMS_SUPPORT_2_SKYPE" description="JMS_SUPPORT_2_SKYPE_DESC"></field>
			<field name="aim2" type="text" default="" label="JMS_SUPPORT_2_AIM" description="JMS_SUPPORT_2_AIM_DESC"></field>
			<field name="msn2" type="text" default="" label="JMS_SUPPORT_2_MSN" description="JMS_SUPPORT_2_MSN_DESC"></field>
			<field name="icq2" type="text" default="" label="JMS_SUPPORT_2_ICQ" description="JMS_SUPPORT_2_ICQ_DESC"></field>
			<field name="tel2" type="text" default="" label="JMS_SUPPORT_2_TELEPHONE" description="JMS_SUPPORT_2_TELEPHONE_DESC"></field>
			<field name="email2" type="text" default="" label="JMS_SUPPORT_2_EMAIL" description="JMS_SUPPORT_2_EMAIL_DESC"></field>		
		</fieldset>	
		
