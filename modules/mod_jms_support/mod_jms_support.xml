			<field name="support3" type="text" default="" label="JMS_SUPPORT_3_SUPPORT_NAME" description="JMS_SUPPORT_3_SUPPORT_NAME_DESC"></field>
			<field name="yahoo3" type="text" default="" label="JMS_SUPPORT_3_YAHOO" description="JMS_SUPPORT_3_YAHOO_DESC"></field>			
			<field name="skype3" type="text" default="" label="JMS_SUPPORT_3_SKYPE" description="JMS_SUPPORT_3_SKYPE_DESC"></field>
			<field name="aim3" type="text" default="" label="JMS_SUPPORT_3_AIM" description="JMS_SUPPORT_3_AIM_DESC"></field>
			<field name="msn3" type="text" default="" label="JMS_SUPPORT_3_MSN" description="JMS_SUPPORT_3_MSN_DESC"></field>
			<field name="icq3" type="text" default="" label="JMS_SUPPORT_3_ICQ" description="JMS_SUPPORT_3_ICQ_DESC"></field>
			<field name="tel3" type="text" default="" label="JMS_SUPPORT_3_TELEPHONE" description="JMS_SUPPORT_3_TELEPHONE_DESC"></field>
			<field name="email3" type="text" default="" label="JMS_SUPPORT_3_EMAIL" description="JMS_SUPPORT_3_EMAIL_DESC"></field>			
		</fieldset>	
		
		<fieldset name="Supporter4">	
			<field name="support4" type="text" default="" label="JMS_SUPPORT_4_SUPPORT_NAME" description="JMS_SUPPORT_4_SUPPORT_NAME_DESC"></field>
			<field name="yahoo4" type="text" default="" label="JMS_SUPPORT_4_YAHOO" description="JMS_SUPPORT_4_YAHOO_DESC"></field>			
			<field name="skype4" type="text" default="" label="JMS_SUPPORT_4_SKYPE" description="JMS_SUPPORT_4_SKYPE_DESC"></field>
			<field name="aim4" type="text" default="" label="JMS_SUPPORT_4_AIM" description="JMS_SUPPORT_4_AIM_DESC"></field>
			<field name="msn4" type="text" default="" label="JMS_SUPPORT_4_MSN" description="JMS_SUPPORT_4_MSN_DESC"></field>
