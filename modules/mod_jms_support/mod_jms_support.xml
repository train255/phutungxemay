<version>2.0</version> 
<description>
<![CDATA[<span style="color: #008000;"><strong>The Jms Support Online module displays online status for SKYPE, MSN, AIM, ICQ, YAHOO and allow user contact with supporter by click on the status. You can check here</strong></span><br /></span>]]>
</description>
<files>
	<filename module="mod_jms_support">mod_jms_support.php</filename>
	<filename>helper.php</filename>
	<filename>index.html</filename>
	<filename>sendmessage.php</filename>
	<folder>tmpl</folder>
	<folder>images</folder>
	<filename>en-GB.mod_jms_support.ini</filename>
</files>
<languages>
	<language tag="en-GB">en-GB.mod_jms_support.ini</language>
</languages>


<config>
	<fields name="params">
		<fieldset name="basic">
			<field name="supportername" type="radio" default="1" class="btn-group" label="JMS_SUPPORT_BASIC_SUPPORTER_NAME"	description="JMS_SUPPORT_BASIC_SUPPORTER_NAME_DESC">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>			
			<field name="display" class="btn-group" type="radio" default="1" label="JMS_SUPPORT_BASIC_WITH_ADDRESS" description="JMS_SUPPORT_BASIC_WITH_ADDRESS_DESC">			
				<option value="0">Icon only</option>
				<option value="1">With address</option>
			</field>
			<field name="cf_yahoo" type="radio" class="btn-group" default="1" label="JMS_SUPPORT_BASIC_SHOW_YAHOO_STATUS" description="JMS_SUPPORT_BASIC_SHOW_YAHOO_STATUS_DESC">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</field>
			<field class="btn-group" name="cf_skype" type="radio" default="1" label="JMS_SUPPORT_BASIC_SHOW_SKYPE_STATUS" description="JMS_SUPPORT_BASIC_SHOW_SKYPE_STATUS_DESC">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</field>
			<field class="btn-group" name="cf_aim" type="radio" default="1" label="JMS_SUPPORT_BASIC_SHOW_AIM_STATUS" description="JMS_SUPPORT_BASIC_SHOW_AIM_STATUS_DESC">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</field>
			<field class="btn-group" name="cf_msn" type="radio" default="1" label="JMS_SUPPORT_BASIC_SHOW_MSN_STATUS" description="JMS_SUPPORT_BASIC_SHOW_MSN_STATUS_DESC">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</field>
			<field class="btn-group" name="cf_icq" type="radio" default="1" label="JMS_SUPPORT_BASIC_SHOW_ICQ_STATUS" description="JMS_SUPPORT_BASIC_SHOW_ICQ_STATUS_DESC">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</field>
			<field class="btn-group" name="cf_tel" type="radio" default="1" label="JMS_SUPPORT_BASIC_SHOW_TELEPHONE" description="JMS_SUPPORT_BASIC_SHOW_TELEPHONE_DESC">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</field>
			<field class="btn-group" name="cf_email" type="radio" default="1" label="JMS_SUPPORT_BASIC_SHOW_EMAIL" description="JMS_SUPPORT_BASIC_SHOW_EMAIL_DESC">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</field>
		</fieldset>
		
		<fieldset name="Supporter1">	
			<field name="support1" type="text" default="" label="JMS_SUPPORT_1_SUPPORT_NAME" description="JMS_SUPPORT_1_SUPPORT_NAME_DESC"></field>
			<field name="yahoo1" type="text" default="" label="JMS_SUPPORT_1_YAHOO" description="JMS_SUPPORT_1_YAHOO_DESC"></field>			
			<field name="skype1" type="text" default="" label="JMS_SUPPORT_1_SKYPE" description="JMS_SUPPORT_1_SKYPE_DESC"></field>
			<field name="aim1" type="text" default="" label="JMS_SUPPORT_1_AIM" description="JMS_SUPPORT_1_AIM_DESC"></field>
			<field name="msn1" type="text" default="" label="JMS_SUPPORT_1_MSN" description="JMS_SUPPORT_1_MSN_DESC"></field>
			<field name="icq1" type="text" default="" label="JMS_SUPPORT_1_ICQ" description="JMS_SUPPORT_1_ICQ_DESC"></field>
			<field name="tel1" type="text" default="" label="JMS_SUPPORT_1_TELEPHONE" description="JMS_SUPPORT_1_TELEPHONE_DESC"></field>
			<field name="email1" type="text" default="" label="JMS_SUPPORT_1_EMAIL" description="JMS_SUPPORT_1_EMAIL_DESC"></field>		
		</fieldset>	
		
